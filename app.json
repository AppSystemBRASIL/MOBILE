{
  "expo": {
    "name": "xcar-corretora",
    "slug": "xcar-corretora",
    "scheme": "xcar-corretora",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 2
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xcarcorretora.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.xcarcorretora.app"
    }
  }
}
